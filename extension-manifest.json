{
    "manifestVersion": 1,
    "id": "cake-build-tasks",
    "name": "Cake Build Tasks",
    "version": "0.0.1",
    "publisher": "cake-build",
    "description": "Tools for building with Cake.",
    "categories": [
        "Build and release"
    ],
    "icons": {
        "default": "extension-icon.png"
    },
    "files": [
        {
            "path": "Task"
        }
    ],
    "content": {
        "details":
        {
            "path": "extension-details.md"
        }
    },
    "links": {
        "home": {
            "uri": "http://cakebuild.net"
        },
        "getstarted": {
            "uri": "http://cakebuild.net/docs/build-systems/vso"
        },
        "learn": {
            "uri": "http://cakebuild.net/docs"
        },
        "issues": {
            "uri": "https://github.com/cake-build/cake-vso/issues"
        },
        "license": {
            "uri": "https://github.com/cake-build/cake-vso/blob/develop/LICENSE"
        },
        "repository": {
            "uri": "http://github.com/cake-build/cake-vso"
        }
    },
    "branding": {
        "color": "rgb(102, 47, 24)",
        "theme": "dark"
    },
    "contributions": [
        {
            "id": "cake-build-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Task"
            }
        }
    ]
}
